{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://json-schema.org/draft-07/schema#",
	"title": "The response from the https://server1.api-football.com/seasons endpoint.",
	"type": "object",
	"required": ["api"],
	"properties": {
		"api": {
			"type": "object",
			"required": ["results",
						 "seasons"],
			"properties": {
				"results": {
					"type": "integer"
				},
				"seasons": {
					"type": "array",
					"items": {
						"type": "integer"
					}
				}
			}
		}
	}
}